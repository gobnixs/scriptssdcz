    function refreshOVHSocket() {
        try{MySocket.connect()}catch(error){console.error("Failed to re-connect to OVH WebSocket, can't refresh bots service")}
        sliderBots.value = `${MySocket.player.decoded.botsMax}`
        window.sliderValue = sliderBots.value;
    } // Re-initiator OVH Socket
    function sliderOVHReader() {
        activeBots.innerHTML = `${sliderBots.value}`;
        console.log(`Changed maximum spawned bots to ${sliderBots.value}`)
        window.sliderValue = sliderBots.value;
        try{MySocket.GUI.update()}catch(error){console.error("Failed to update OVH GUI on slider changes")}
    } // Reader for OVH slider changes
    function removeAdvertisement() {
        try{document.getElementsByClassName('iziModal-overlay')[0].remove()}catch(error){console.warn("No 'iziModal-overlay' element class was detected (dark advertisement overlay)")} // Removing shit advertisements overlay by Neo
        try{document.getElementById('rekmodal').remove();}catch(error){console.warn("No 'rekmodal' element id was detected (advertisement banner)")}  // Removing shit advertisements by Neo
        try{document.getElementById('botnum').remove();}catch(error){console.warn("No 'botnum' element id was detected")} // Removing bugged bot enumeration from OVH GUI
    } // Remove advertisement and bugged OVH elements
    function setTargetPosition(i,e,t){
        app.fcontroller.controllers.spect16x._clients[i].targetX=e-app.fcontroller.controllers.spect16x._clients[i].mapOffsetX,app.fcontroller.controllers.spect16x._clients[i].targetY=t-app.fcontroller.controllers.spect16x._clients[i].mapOffsetY,app.fcontroller.controllers.spect16x._clients[i].targetDistance=Math.round(Math.sqrt(Math.pow(app.fcontroller.controllers.spect16x._clients[i].playerX-app.fcontroller.controllers.spect16x._clients[i].targetX,2)+Math.pow(app.fcontroller.controllers.spect16x._clients[i].playerY-app.fcontroller.controllers.spect16x._clients[i].targetY,2)))
    } // Move the region clients to coordinate x and y
    function logClient (ID,msg){
        console.log(`[`+`%cClient ${ID}`+`%c]: ${msg} %c-> greasieball#0856`,`color: #9A48F0`,null,`color: red`)
    }
    function initializeClients (clients){
        for (let i=0;i<clients;i++) {
            try{
                app.initClient(3)
                logClient(i,`Successfully initiated`)
                app.clients.region[i].profile._nick = '么 Spectator'
                app.clients.region[i].playerNick = '么 Spectator'
                logClient(i,`Successfully renamed to ${app.clients.region[i].profile._nick}`)
            }catch(error){logClient(i,`Failed initialization`)}
        }
        app.fcontroller.controllers.spect16x._clients = app.clients.region
    }
    function destroyClients (){
        while(app.fcontroller.controllers.spect16x._clients.length !== 0) {
            try{
                var clientID = app.fcontroller.controllers.spect16x._clients[0].ID
                app.destroyClient(app.fcontroller.controllers.spect16x._clients[0])
                app.destroyClient(app.clients.region[0])
                logClient(clientID,`Successfully destroyed`)
            }catch(error){logClient(clientID,`Failed to destroyed`)}
        }
    }
    function signature() {
        console.log(sign,`color: #BF40BF`)
    }

